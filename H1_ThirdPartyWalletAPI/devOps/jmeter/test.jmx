            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.*
import org.apache.commons.codec.binary.Base64


def responseStr = prev.getResponseDataAsString()
def jsonSlurper = new JsonSlurper()
def response = jsonSlurper.parseText(responseStr)

log.info(&quot;responseStr: &quot; + responseStr);
String[] arr=response.token.split(&quot;\\.&quot;)
String actualvalue=arr[1].replaceAll(&quot;-&quot;,&quot;+&quot;)
actualvalue=actualvalue.replaceAll(&quot;_&quot;,&quot;/&quot;)
byte[] decoded_response = Base64.decodeBase64(actualvalue)

def jwtStr = new String(decoded_response)
log.info(&quot;jwtStr: &quot; + jwtStr);
// {&quot;email&quot;:&quot;&quot;,&quot;FullName&quot;:&quot;&quot;,&quot;nameid&quot;:&quot;&quot;,&quot;http://schemas.microsoft.com/ws/2008/06/identity/claims/role&quot;:&quot;admin&quot;,&quot;exp&quot;:1675561163,&quot;iss&quot;:&quot;One.com&quot;,&quot;aud&quot;:&quot;MY&quot;}

def jwt = jsonSlurper.parseText(jwtStr)

def jwtTmp = [
	email: &quot;&quot;,
	FullName: &quot;&quot;,
	nameid: &quot;&quot;,
	&quot;http://schemas.microsoft.com/ws/2008/06/identity/claims/role&quot;: &quot;&quot;,
	exp: &quot;&quot;,
	iss: &quot;&quot;,
	aud: &quot;&quot;
]

if (jwtTmp.keySet().containsAll(jwt.keySet())) {
	vars.put(&apos;jwtToken&apos;, response.token);
	log.info(&quot;var:jwtToken: &quot; + vars.get(&apos;jwtToken&apos;));
	return
}
AssertionResult.setFailure(true);
AssertionResult.setFailureMessage(&quot;jwt token 格式錯誤&quot;);
</stringProp>
